@inherits BaseQuestionComponent<DateTime>

<div class="form-group row justify-content-around">
    <label class="col-lg-2">@Label</label>
    <input type="datetime-local" class="form-control col-lg-9" @oninput="SetValue" />
</div>

@code {
    protected override bool TryParse(string original, out DateTime result) => DateTime.TryParse(original, out result);
}